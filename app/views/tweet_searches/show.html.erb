
<table class='table table-striped table-condensed'>
  <thead>
    <tr>
      <th>Text</th>
      <th>Tweeter</th>
    </tr>
  </thead>
  <tbody>
    <% @tweet_search.results.each do |tweet| %>
      <tr>
        <td><%= tweet.text%></td>
        <td><%= tweet.user.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
